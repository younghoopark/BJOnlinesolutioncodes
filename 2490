#include <iostream>

using namespace std;

int main()
{
	int a[4];
	int cnt;
	for (int i = 0; i < 3; i++)
	{
		cin >> a[0] >> a[1] >> a[2] >> a[3];
		cnt = 0;
		for (int j = 0; j < 4; j++)
		{
			if (a[j] == 1) cnt++;
		}
		if (cnt == 0)
		{
			cout << 'D' << endl;
		}
		else if (cnt == 1)
		{
			cout << 'C' << endl;
		}
		else if (cnt == 2)
		{
			cout << 'B' << endl;
		}
		else if (cnt == 3)
		{
			cout << 'A' << endl;
		}
		else
		{
			cout << 'E' << endl;
		}
	}
	return 0;
}
